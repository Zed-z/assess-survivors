config_version=5

[application]

config/name="Assess Survivors"
config/version="DEVEL"
run/main_scene="uid://cksd53ocdjjc5"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.5", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="uid://buuaf3wdlshc0"

[autoload]

AudioManager="*res://globals/audio_manager/audio_manager.tscn"
UiAudioPlayer="*res://globals/ui_audio_player/ui_audio_player.tscn"
SceneManager="*res://globals/scene_manager/scene_manager.tscn"
GlobalInfo="*res://globals/global_info/global_info.gd"
ScreenManager="*res://globals/screen_manager/screen_manager.tscn"
SettingsManager="*res://globals/settings_manager/settings_manager.tscn"
ObjectManager="*res://globals/object_manager/object_manager.tscn"
Utils="*res://globals/utils.gd"
PauseManager="*res://globals/pause_manager.gd"
MusicPlayer="*res://globals/music_player/music_player.tscn"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/addons/SimpleFormatOnSave/plugin.cfg", "res://addons/gdUnit4/plugin.cfg")

[file_customization]

folder_colors={
"res://globals/assess_criterion_manager/": "purple",
"res://objects/player/Stats/": "blue",
"res://test/unit/": "teal",
"res://vfx/": "green"
}

[gdunit4]

settings/common/update_notification_enabled=false

[global_group]

